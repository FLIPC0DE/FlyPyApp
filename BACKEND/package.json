{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext js --exec \"node src/app.js\"",
    "start": "node src/app.js",
    "prisma:migrate": "npx prisma migrate dev --name init_extended_schema",
    "prisma:seed": "node prisma/seed.js",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset --force",
    "test": "jest --config=jest.config.js",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "lint": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.10",
    "openid-client": "^6.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.18.0",
    "supertest": "^7.1.4"
  }
}
